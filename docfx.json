{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Qwen KCC On-device Pipeline",
      "_appTitle": "Qwen KCC On-device Pipeline",
      "_appFooter": "(C) 2025 URAV ADVANCED LEARNING SYSTEMS PRIVATE LIMITED, licensed under the <a href=\"https://github.com/Uralstech/Qwen-KCC-On-Device-Pipeline/blob/main/LICENSE\">Apache License, Version 2.0</a>.",
      "_enableSearch": false
    },
    "sitemap": {
      "baseUrl": "https://uralstech.github.io/Qwen-KCC-On-Device-Pipeline",
      "priority": 1.0,
      "changefreq": "daily"
    }
  }
}